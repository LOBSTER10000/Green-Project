<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE mapper
                PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sunny.green.dao.AdminDao">

    <insert id="insertPro">
        insert into product(pro_name, pro_content, pro_point, pro_from) values (#{pro_name}, #{pro_content}, #{pro_point}, #{pro_from})
    </insert>

    <select id="selectPro" resultType="com.sunny.green.vo.ProductVo">
        select * from product where pro_num = 1
    </select>


</mapper>